Algoritmo "soma"

Var

   valor1, valor2 : inteiro
   soma : inteiro
   
Inicio

      escreval("Digite o valor de X:")
      leia(valor1)
      escreval("Digite o valor de Y:")
      leia(valor2)
      
      soma <- (valor1 + valor2)
      
      escreval("O resultado da soma entre X e Y é de", soma)

Fimalgoritmo